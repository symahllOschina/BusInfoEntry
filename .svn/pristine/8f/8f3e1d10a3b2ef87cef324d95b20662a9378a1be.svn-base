<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    >
    <include
        layout="@layout/main_toolbar_layout">
    </include>
    <TextView
        android:id="@+id/info_failure_hintText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/failure_hints"
        android:textSize="@dimen/TextSize14"
        android:textColor="@color/red_FF1200"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:background="@color/yellow_E4C9AF"
        android:visibility="gone"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/white_f2f2f2"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingBottom="15dp"
            android:id="@+id/parent_myLayout"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:textColor="@color/grey_666666"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="微信"
                    android:textSize="@dimen/TextSize16"
                    android:textColor="@color/grey_666666"
                    android:layout_centerVertical="true"
                    ></TextView>
                <!-- Switch常用属性：
                textOn：控件打开时显示的文字
                textOff：控件关闭时显示的文字
                thumb：控件开关的图片
                track：控件开关的轨迹图片
                typeface：设置字体类型
                switchMinWidth：开关最小宽度
                switchPadding：设置开关 与文字的空白距离
                switchTextAppearance：设置文本的风格
                checked：设置初始选中状态
                splitTrack：是否设置一个间隙，让滑块与底部图片分隔（API 21及以上）
                showText：设置是否显示开关上的文字（API 21及以上） -->
                <Switch
                    android:id="@+id/rate_info_wxSwitch"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:switchMinWidth="20dp"
                    android:textOn="开"
                    android:textOff="关"
                    android:thumb="@drawable/thumb"
                    android:track="@drawable/track"
                    android:layout_alignParentRight="true"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_centerVertical="true"
                    android:showText="true"
                    />
            </RelativeLayout>
            <View
                android:id="@+id/rate_info_wxFGX"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/white_ffffff"
                ></View>
            <LinearLayout
                android:id="@+id/rate_info_wxLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/white_ffffff"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:gravity="center_vertical"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:visibility="gone"
                    >
                    <TextView
                        style="@style/info_text_xinghao"
                        android:text="*"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="center_vertical"
                        />
                    <TextView
                        style="@style/info_text_title"
                        android:text="微信账号："
                        android:ems="6"
                        android:layout_gravity="center_vertical"
                        />
                    <EditText
                        android:id="@+id/rate_info_wxContidText"
                        style="@style/info_text_etText_bgf3f7fd"
                        android:hint="联系人微信账号"
                        android:inputType="textVisiblePassword"
                        android:digits="0123456789abcdefghigklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                        />
                </LinearLayout>
                <View
                    style="@style/divider_line1dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:visibility="gone"
                    ></View>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:background="@color/white_ffffff"
                    >
                    <TextView
                        style="@style/info_text_xinghao"
                        android:text="*"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="center_vertical"
                        android:visibility="invisible"
                        />
                    <TextView
                        style="@style/info_text_title"
                        android:text="微信费率："
                        android:ems="6"
                        android:layout_gravity="center_vertical"
                        />
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/rate_info_wxRateReduce"
                            style="@style/info_rate_imgView"
                            android:src="@drawable/rate_reduce_icon"
                            />
                        <EditText
                            android:id="@+id/rate_info_wxRate"
                            style="@style/info_rate_etText"
                            />
                        <ImageView
                            android:id="@+id/rate_info_wxRateAdd"
                            style="@style/info_rate_imgView"
                            android:src="@drawable/rate_add_icon"
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:textColor="@color/grey_666666"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="支付宝"
                    android:textSize="@dimen/TextSize16"
                    android:textColor="@color/grey_666666"
                    android:layout_centerVertical="true"
                    ></TextView>
                <!-- Switch常用属性：
                textOn：控件打开时显示的文字
                textOff：控件关闭时显示的文字
                thumb：控件开关的图片
                track：控件开关的轨迹图片
                typeface：设置字体类型
                switchMinWidth：开关最小宽度
                switchPadding：设置开关 与文字的空白距离
                switchTextAppearance：设置文本的风格
                checked：设置初始选中状态
                splitTrack：是否设置一个间隙，让滑块与底部图片分隔（API 21及以上）
                showText：设置是否显示开关上的文字（API 21及以上） -->
                <Switch
                    android:id="@+id/rate_info_aliSwitch"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:switchMinWidth="20dp"
                    android:textOn="开"
                    android:textOff="关"
                    android:thumb="@drawable/thumb"
                    android:track="@drawable/track"
                    android:layout_alignParentRight="true"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_centerVertical="true"
                    android:showText="true"
                    />
            </RelativeLayout>
            <View
                android:id="@+id/rate_info_aliFGX"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/white_ffffff"
                android:visibility="gone"
                ></View>
            <LinearLayout
                android:id="@+id/rate_info_aliLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/white_ffffff"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:gravity="center_vertical"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:visibility="gone"
                    >
                    <TextView
                        style="@style/info_text_xinghao"
                        android:text="*"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="center_vertical"
                        />
                    <TextView
                        style="@style/info_text_title"
                        android:text="PID："
                        android:ems="6"
                        android:layout_gravity="center_vertical"
                        />
                    <EditText
                        android:id="@+id/rate_info_aliSourceText"
                        style="@style/info_text_etText_bgf3f7fd"
                        android:hint="合作伙伴在支付宝的PID"
                        android:inputType="textVisiblePassword"
                        android:digits="0123456789abcdefghigklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                        />
                </LinearLayout>
                <View
                    style="@style/divider_line1dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:visibility="gone"
                    ></View>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:background="@color/white_ffffff"
                    android:layout_marginTop="0dp"
                    >
                    <TextView
                        style="@style/info_text_xinghao"
                        android:text="*"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="center_vertical"
                        android:visibility="invisible"
                        />
                    <TextView
                        style="@style/info_text_title"
                        android:text="支付宝费率："
                        android:ems="6"
                        android:layout_gravity="center_vertical"
                        />
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/rate_info_aliRateReduce"
                            style="@style/info_rate_imgView"
                            android:src="@drawable/rate_reduce_icon"
                            />
                        <EditText
                            android:id="@+id/rate_info_aliRate"
                            style="@style/info_rate_etText"
                            />
                        <ImageView
                            android:id="@+id/rate_info_aliRateAdd"
                            style="@style/info_rate_imgView"
                            android:src="@drawable/rate_add_icon"
                            />
                    </LinearLayout>
                </LinearLayout>
                <View
                    style="@style/divider_line1dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:visibility="gone"
                    ></View>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:gravity="center_vertical"
                    android:visibility="gone"
                    >
                    <TextView
                        style="@style/info_text_xinghao"
                        android:text="*"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="center_vertical"
                        />
                    <TextView
                        style="@style/info_text_title"
                        android:text="经营类目："
                        android:ems="6"
                        android:layout_gravity="center_vertical"
                        />
                    <TextView
                        android:id="@+id/rate_info_aliCtgyidText"
                        style="@style/info_text_etText_bgf3f7fd"
                        android:hint="请选择"
                        android:drawableRight="@drawable/spinner_icon_xiao"
                        android:gravity="center_vertical"
                        android:ellipsize="end"
                        android:maxLines="2"
                        />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="费率：（单位‰）"
                android:textSize="@dimen/TextSize16"
                android:textColor="@color/grey_666666"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="horizontal"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:background="@color/white_ffffff"
                android:layout_marginTop="10dp"
                >
                <TextView
                    style="@style/info_text_title"
                    android:text="翼支付费率："
                    android:ems="6"
                    android:layout_gravity="center_vertical"
                    />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center"
                    >
                    <ImageView
                        android:id="@+id/rate_info_yzfRateReduce"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_reduce_icon"
                        />
                    <EditText
                        android:id="@+id/rate_info_yzfRate"
                        style="@style/info_rate_etText"
                        />
                    <ImageView
                        android:id="@+id/rate_info_yzfRateAdd"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_add_icon"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="horizontal"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:background="@color/white_ffffff"
                android:layout_marginTop="10dp"
                >
                <TextView
                    style="@style/info_text_title"
                    android:text="借记卡费率："
                    android:ems="6"
                    android:layout_gravity="center_vertical"
                    />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center"
                    >
                    <ImageView
                        android:id="@+id/rate_info_jjkRateReduce"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_reduce_icon"
                        />
                    <EditText
                        android:id="@+id/rate_info_jjkRate"
                        style="@style/info_rate_etText"
                        />
                    <ImageView
                        android:id="@+id/rate_info_jjkRateAdd"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_add_icon"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="horizontal"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:background="@color/white_ffffff"
                android:layout_marginTop="10dp"
                >
                <TextView
                    style="@style/info_text_title"
                    android:text="贷记卡费率："
                    android:ems="6"
                    android:layout_gravity="center_vertical"
                    />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center"
                    >
                    <ImageView
                        android:id="@+id/rate_info_djkRateReduce"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_reduce_icon"
                        />
                    <EditText
                        android:id="@+id/rate_info_djkRate"
                        style="@style/info_rate_etText"
                        />
                    <ImageView
                        android:id="@+id/rate_info_djkRateAdd"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_add_icon"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="horizontal"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:background="@color/white_ffffff"
                android:layout_marginTop="10dp"
                >
                <TextView
                    style="@style/info_text_title"
                    android:text="银联费率："
                    android:ems="6"
                    android:layout_gravity="center_vertical"
                    />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center"
                    >
                    <ImageView
                        android:id="@+id/rate_info_ylRateReduce"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_reduce_icon"
                        />
                    <EditText
                        android:id="@+id/rate_info_ylRate"
                        style="@style/info_rate_etText"
                        />
                    <ImageView
                        android:id="@+id/rate_info_ylRateAdd"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_add_icon"
                        />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="借记卡封顶值：（单位元）"
                android:textSize="@dimen/TextSize16"
                android:textColor="@color/grey_666666"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="horizontal"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:background="@color/white_ffffff"
                >
                <TextView
                    style="@style/info_text_title"
                    android:text="借记卡封顶值："
                    android:ems="6"
                    android:layout_gravity="center_vertical"
                    />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center"
                    >
                    <ImageView
                        android:id="@+id/rate_info_bankReserveReduce"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_reduce_icon"
                        />
                    <EditText
                        android:id="@+id/rate_info_bankReserve"
                        style="@style/info_rate_etText"
                        />
                    <ImageView
                        android:id="@+id/rate_info_bankReserveAdd"
                        style="@style/info_rate_imgView"
                        android:src="@drawable/rate_add_icon"
                        />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="资质材料证明："
                android:textSize="@dimen/TextSize16"
                android:textColor="@color/grey_666666"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                />
            <include layout="@layout/photo_info_layout" />
        </LinearLayout>
    </ScrollView>
    <Button
        android:id="@+id/account_rate_btSubmit"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="提    交"
        android:textSize="@dimen/TextSize18"
        android:textColor="@color/white_ffffff"
        android:background="@color/blue_409EFF"
        />
</LinearLayout>
